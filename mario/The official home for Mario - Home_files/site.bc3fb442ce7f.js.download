        if (typeof item.classes === 'object' && item.classes.length > 0) {
          classList += ' ' + item.classes.join(' ');
        }
        if (item.rootType === 'tile') {
          tile_type = item.type;
          tile_importance = ['null', 'featured', 'two', 'one'].indexOf(tile_type);
          classList = classList + " importance-" + tile_importance;
          if (tile_type === 'two') {
            classList += " subposition-" + item.subposition + " direction-" + item.direction;
          }
        } else if (item.rootType === 'theme') {
          classList += " theme1x1";
        }
        classList += " stagger-" + item.stagger;
        return classList;
      };

      /*
        public **getStyleObjForItem**
        -------------------------
        Get the style object for the supplied item.
        __Properties__
        - *object* __item__ The tile record to get the styles for.
        __Returns__
        - *object* __styles__ An object representing the styles to apply to the element
