        character.src = "/static/assets/img/surprises/" + random_character.file;
        if (random_character.file2) {
          change_img = new Image();
          change_img.src = "/static/assets/img/surprises/" + random_character.file2;
        }
        cont.appendChild(character);
        angular.element(cont).addClass('backtotop-' + random_character.name);
        fairydust = function() {
          var createFairyDustTween, dot, i, results, tlfd;
          tlfd = new TimelineLite();
          i = 25;
          createFairyDustTween = function(dot) {
            var beziervalues;
            dot.style.height = (Math.floor(Math.random() * 10)) + "px";
            dot.style.width = dot.style.height;
            dot.style.opacity = 1;
            beziervalues = {
              values: [
                {
                  x: Math.random() * 50 - Math.random() * 30,
                  y: Math.random() * 150
                }
              ]
            };
            TweenLite.to(dot, 0.5 + (Math.random() * 1), {
              bezier: beziervalues,
              opacity: 0,
              scaleX: 0,
              scaleY: 0,
              ease: Cubic.easeIn,
              onComplete: function() {
                return createFairyDustTween(dot);
              },
              paused: false
            });
            return TweenLite.set(dot, {
              x: 0,
              y: 0
            });
          };
          results = [];
          while (i >= 0) {
            dot = document.createElement('div');
            dot.className = 'dot';
            cont.appendChild(dot);
            createFairyDustTween(dot);
            results.push(i--);
          }
          return results;
        };
        if (random_character.name === "tatl") {
          fairydust();
        }
        if (navigator.userAgent.match(/(iPod|iPhone|iPad|Android)/)) {
          scrollanim = false;
        } else {
          scrollanim = true;
          elem.addClass('allowhover');
        }
        return elem[0].addEventListener('click', function(e) {
          var character_exit_delay, character_exit_position_x, character_exit_position_y, character_fall_time, character_rotation, distance, fairyTween, ref, ref1, shyguyTween, tl, winheight;
          e.preventDefault();
          winheight = window.innerHeight;
          elem.addClass('transforming');
          distance = character.getBoundingClientRect();
          character_fall_time = 1.5;
          if (random_character.name === "peach") {
            character_fall_time *= 2;
          }
          character_exit_position_y = winheight;
          if ((ref = random_character.name) === "kirby-rocket" || ref === "tatl") {
            character_exit_position_y = winheight * -1;
          }
          if ((ref1 = random_character.name) === "shyguy") {
            character_exit_position_y = 0;
          }
          character_exit_position_x = 0;
          if (random_character.name === "shyguy") {
            character_exit_position_x = 500;
          }
          character_rotation = 0;
          if (random_character.name === "shyguy") {
            character_rotation = 12;
          }
          character_exit_delay = "-=1.2";
          if (random_character.name === "diddy") {
            character.src = "/static/assets/img/surprises/" + random_character.file2;
          }
          if (random_character.name === "shyguy") {
            character.src = "/static/assets/img/surprises/" + random_character.file2;
          }
          fairyTween = new TweenLite(cont, 2, {
            bezier: {
              curviness: 1.5,
              values: [
                {
                  x: -100,
                  y: -100
                }, {
                  x: -200,
                  y: 0
                }, {
                  x: -100,
                  y: 100
                }, {
                  x: 0,
                  y: 0
                }, {
                  x: 200,
                  y: -1000
                }
              ]
            },
            ease: Linear.easeNone,
            repeat: 0,
            paused: false
          });
          shyguyTween = new TweenLite(cont, 4, {
            bezier: {
              curviness: 1.5,
              values: [
                {
                  x: -80,
                  y: -30
                }, {
                  x: -150,
                  y: 10
                }, {
                  x: -50,
                  y: 0
                }, {
                  x: 0,
                  y: -10
                }, {
                  x: 1000,
                  y: -300
                }
              ]
            },
            ease: Linear.easeNone,
            repeat: 0,
            paused: false
          });
          fairyTween.timeScale(1);
          tl = new TimelineLite();
          tl.to(cont, 0.3, {
            y: -distance.top / 2,
            x: 0,
            height: 250,
            ease: Linear.easeNone
          });
          tl.set(cont, {
            y: 0,
            width: '8em',
            top: distance.top / 2,
            position: 'fixed'
          });
          if (scrollanim) {
            tl.to(window, 1.5, {
              scrollTo: {
                y: 0
              },
              ease: Cubic.easeOut
            }, 0.3);
          } else {
            tl.to(window, 1.5, {
              onStart: function() {
                return window.scrollTo(0, 0);
              }
            }, 0.3);
          }
          if (random_character.name === "tatl") {
            tl.to(cont, 1.3, {
              y: -60,
