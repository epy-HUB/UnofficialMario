/*
    Angular Directive - Play Character friends list
    =======================================
  - *Author*          liamegan
  - *email*           liam@wethecollective.com
  - *Created*         2014-06-09 07:40:42
  - *namespace*       window
  - *Requirements*    n/a
  - *Description*     An angular directive for creating the friends list on character pages
  - *Edited by*       liamegan
  - *Edited*          2014-10-20 14:13:46
  - *Version*         1.0
 */
(function(app, swipe) {
  return app.directive('playCharacterFriends', function() {
    return {
      restrict: 'E',
      replace: true,
      template: window.wtc.TEMPLATES.playCharacterFriends,
      link: function(scope, el, attrs) {
        var elementWidth, friensel, resizeCB, swiper, wrapper;
        scope.headline = attrs.headline;
        elementWidth = 97;
        wrapper = el[0].querySelector('.container');
        friensel = el[0].querySelector('.switcher');
        swiper = new swipe.Swiper(friensel, {
          navContainer: friensel.querySelector('nav'),
          navLeft: friensel.querySelector('.nav-left'),
          navRight: friensel.querySelector('.nav-right'),
          wrapperSelector: '.container',
          itemWrapperSelector: 'ul',
          fractionBeforeScroll: 6
        });
        resizeCB = function() {
          var BP, newWidth;
          BP = window.wtc.responsive.Breakpoints.getBreakpoint();
          friensel.style.width = '';
          wrapper.style.width = '';
          if (BP === 'small') {
            return;
          }
          if (friensel.offsetWidth < 1000) {
            newWidth = Math.floor(wrapper.offsetWidth / 97) * 97;
            return wrapper.style.width = newWidth + "px";
          }
        };
        window.addEventListener('resize', resizeCB);
        setTimeout((function(_this) {
          return function() {
            swiper.onResize();
            return resizeCB();
          };
        })(this), 100);
        return setTimeout((function(_this) {
          return function() {
            swiper.onResize();
            return resizeCB();
          };
        })(this), 1000);
      }
    };
  });
})(window.PlayNintendo.ngApp, window.Swipe);


