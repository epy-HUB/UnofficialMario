      restrict: 'AE',
      replace: true,
      template: window.wtc.TEMPLATES.playGridItem,
      link: function(scope, el, attrs) {
        setTimeout(function() {
          var link, newClass, ngrid, swiper;
          ngrid = el[0].parentNode;
          swiper = ngrid.parentNode.parentNode.parentNode;
          link = el[0].querySelector('a');
          newClass = 'run';
          link.addEventListener('click', function(e) {
            if (ngrid.className.indexOf('activity-externallink') !== -1) {
              try {
                Omniture.exitLink(this.href);
              } catch (error) {
                e = error;
                console.log('error in external link attribution');
              }
              return true;
            }
            console.log('-----');
            console.log(ngrid.parentNode.className);
            console.log('-----');
            if (ngrid.className.indexOf('activity-externallink') !== -1) {
              return true;
            }
            if (ngrid.className.indexOf('special') !== -1) {
              return true;
            }
            e.preventDefault();
            if (swiper.className.indexOf('click-disabled') > -1) {
              return false;
            }
            if (ngrid.classList) {
              ngrid.classList.add(newClass);
            } else {
              el.className += ' ' + newClass;
