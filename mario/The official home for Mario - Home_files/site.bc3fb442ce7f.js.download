        if (randomTileInserted) {
          return;
        }
        randomTileInserted = true;
        $scope.data.results.splice(position, 0, {
          "id": null,
          "rootType": 'special',
          "content": {
            "type": "Special.Random"
          },
          "type": "random"
        });
      };

      /*
        public **getClassListForItem**
        -------------------------
        Get the class list for the supplied item. This combines the priotity, type and slug
        __Properties__
        - *object* __item__ The tile record to get the class list for.
        __Returns__
