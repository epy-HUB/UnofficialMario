(function(app) {
  return app.directive('playSearchBox', function() {
    return {
      restrict: 'E',
      transclude: true,
      replace: true,
      template: window.wtc.TEMPLATES.playSearchBox,
      link: function($scope, element, attrs) {
        return setTimeout(function() {
          var KEY, _focusIndex, addListeners, checkbox, container, el, focus, form, formResize, input, keyDown, label, logowidth, mobileNavCheckbox, onClick, removeListeners, suggestions, tags, unfocussed;
          el = element[0];
          label = el.getElementsByTagName('label')[0];
          mobileNavCheckbox = document.getElementById('mobilenav');
          logowidth = (document.querySelector('header#top h1') || document.querySelector('header#btt h1')).offsetWidth;
          checkbox = document.getElementById('searchopen');
          form = el.querySelector('#searchform');
          input = form.querySelector('input');
          suggestions = form.querySelector('.suggestions');
          tags = form.querySelector('.tags');
          container = el.parentNode;
          form.addEventListener('submit', function(e) {
            var searchdata;
            searchdata = input.value.toLowerCase();
            window.wtc.Nintendo.Omni.track({
              5: searchdata
            }, [6]);
            return true;
          });
          KEY = {
            UP: 38,
            DOWN: 40,
            ESC: 27
          };
          unfocussed = false;
          _focusIndex = 0;
          addListeners = function() {
            input.addEventListener('keydown', keyDown);
            suggestions.addEventListener('keydown', keyDown);
            return window.addEventListener('click', onClick);
          };
          removeListeners = function() {
            input.addEventListener('keydown', keyDown);
            suggestions.removeEventListener('keydown', keyDown);
            return window.removeEventListener('click', onClick);
          };
          focus = function(index) {
            var as;
            if (index < 0 || $scope.search._length === 0) {
              _focusIndex = 0;
            } else if (index > $scope.search._length) {
              _focusIndex = $scope.search._length;
            } else {
              _focusIndex = index;
            }
            $scope.search.focusIndex = _focusIndex;
            if (_focusIndex > 0) {
              as = suggestions.querySelectorAll('li a');
              if (as[_focusIndex - 1]) {
                as[_focusIndex - 1].focus();
                unfocussed = true;
              } else {
                return;
              }
            } else {
              input.focus();
              unfocussed = false;
            }
          };
          keyDown = function(e) {
            var c;
            c = event.keyCode;
            if (c === KEY.ESC) {
              document.getElementById('searchopen').checked = false;
              return true;
            }
            if (c === KEY.UP || c === KEY.DOWN) {
              if (c === KEY.UP) {
                focus(_focusIndex - 1);
              } else if (c === KEY.DOWN) {
                focus(_focusIndex + 1);
              }
              e.preventDefault();
              return false;
            } else {
              if (unfocussed) {
                focus(0);
              }
              return true;
            }
          };
          onClick = function(e) {
            var _el, isDescendent;
            _el = e.target;
            isDescendent = false;
            while (_el.parentNode) {
              if (_el === el) {
                isDescendent = true;
                break;
              } else {
                _el = _el.parentNode;
              }
            }
            if (isDescendent) {
              return true;
            } else {
              document.getElementById('searchopen').checked = false;
            }
          };
          if (tags) {
            tags.addEventListener('click', function(e) {
              var inputevent, t;
              if (e.target.nodeName.toLowerCase() === 'span') {
                t = e.target.textContent;
              }
              input.value = t;
              inputevent = document.createEvent('HTMLEvents');
              inputevent.initEvent('input', true, false);
              input.dispatchEvent(inputevent);
              input.focus();
            });
          }
          $scope.$watch('search.open', function(v) {
            var siteheader;
            if (v === true) {
              mobileNavCheckbox.checked = false;
              input.focus();
              $scope.clear();
              removeListeners();
              addListeners();
              if (/(iPad|iPhone|iPod)/g.test(navigator.userAgent)) {
                window.scrollTo(0, 0);
                siteheader = document.getElementById('top');
                siteheader.style.position = 'absolute';
                return siteheader.style.top = "0px";
              }
            } else {
              input.blur();
              input.value = '';
              $scope.clear();
              removeListeners();
              if (/(iPad|iPhone|iPod)/g.test(navigator.userAgent)) {
                siteheader = document.getElementById('top');
                siteheader.style.position = '';
                return siteheader.style.top = "";
              }
            }
          });
          formResize = function() {
            var BP, isParents, width;
            BP = window.wtc.responsive.Breakpoints.getBreakpoint(true);
            isParents = attrs.parents === 'true';
            if (isParents) {
              if (BP[0] === 'small') {
                width = Math.ceil(window.innerWidth - label.offsetWidth);
              } else {
                width = Math.ceil(container.offsetWidth - label.offsetWidth - 136 - logowidth);
              }
            } else {
              if (BP[0] === 'small') {
                width = Math.ceil(window.innerWidth - label.offsetWidth);
              } else {
                width = document.getElementById('main-nav').offsetWidth - label.offsetWidth;
              }
            }
            form.style.width = width + "px";
            suggestions.style.width = width + "px";
          };
          window.addEventListener('resize', formResize);
          return formResize();
        }, 0);
      }
    };
  });
})(window.PlayNintendo.ngApp);


/*
  Angular Directive - Play Nintendo search results
  =======================================
  - *Author*          liamegan
  - *email*           liam@wethecollective.com
  - *Created*         2014-06-09 07:40:42
  - *namespace*       window
  - *Requirements*    n/a
  - *Description*     An angular directive for displaying search results
  - *Edited by*       liamegan
  - *Edited*          2014-10-14 10:25:03
  - *Version*         1.0
 */
(function(app) {
  return app.directive('playSearchResults', function() {
    return {
      scope: true,
      restrict: 'C',
      link: function(scope, elem, attrs) {
        var parents, running, scopewatch;
        if (elem.hasClass('parents-search-results')) {
          parents = true;
          scopewatch = 'parents.data';
        } else {
          parents = false;
          scopewatch = 'data';
        }
        running = false;
        scope = scope.$$childTail;
        scope.$watch(scopewatch, function(newData) {
          return setTimeout(function() {
            var _h1, _scope, animate, frame, h1, newtext, searchTerm, starttime, test, text, writeupafter, writingdown;
            if (parents) {
              test = scope.parents && scope.parents.data && scope.parents.data.results;
              _scope = scope.parents;
            } else {
              test = scope && scope.data && scope.data.results;
              _scope = scope;
            }
            if (newData) {
              if (!test || _scope.data.results.length <= 0) {
                if (running) {
                  return;
                }
                running = true;
                _h1 = elem.find('h1');
                h1 = _h1[0];
                searchTerm = _h1.find('strong')[0].innerText;
                text = h1.innerText;
                newtext = "Hmm, we couldn't find " + searchTerm + ", but here’s some other cool stuff.";
                if (parents) {
                  newtext = "We couldn't find any matches for " + searchTerm + ". Try again, or take a look at our most popular posts:";
                }
                starttime = new Date().getTime();
                frame = 0;
                writingdown = true;
                writeupafter = 20;
                animate = function() {
                  var ellapsed, newframe, now;
                  now = new Date().getTime();
                  ellapsed = now - starttime;
                  newframe = Math.floor(ellapsed / 10);
                  if (newframe !== frame) {
                    if (!text.length && writingdown) {
                      writingdown = false;
                      writeupafter += newframe;
                    }
                    if (writingdown) {
                      text = text.substr(0, text.length - 1);
                    } else if (frame >= writeupafter) {
                      text = newtext.substr(0, text.length + 1);
                    }
                    if (text === '') {
                      h1.innerText = '_';
                    } else {
                      h1.innerText = text + '_';
                    }
                    frame = newframe;
                  }
                  if (text !== newtext) {
                    requestAnimationFrame(animate);
                  } else {
                    h1.innerText = text;
                    if (parents) {
                      _scope.loadPopular();
                      delete _scope.params.search;
                    } else {
                      _scope.load();
                      delete _scope.gridparams.search;
                    }
                  }
                };
                return setTimeout(function() {
                  return animate();
                }, 1000);
              }
            }
          }, 1000);
        });
      }
    };
  });
})(window.PlayNintendo.ngApp);


