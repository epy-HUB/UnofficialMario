            }, "-=1.1");
            tl.add(fairyTween, 2, {
              play: true,
              paused: false
            }, "-=0.2");
          } else {
            if (random_character.name === "shyguy") {
              tl.to(cont, 0.8, {
                x: -50,
                y: -20,
                rotation: character_rotation,
                ease: Cubic.easeIn
              }, "-=0.5");
              tl.add(shyguyTween, 2, {
