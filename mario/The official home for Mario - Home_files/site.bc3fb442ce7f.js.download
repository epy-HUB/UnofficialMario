  - *Description*     An angular directive for creating the grid object.
  - *Edited by*       liamegan
  - *Edited*          2015-05-07 16:36:17
  - *Version*         1.0
 */
(function(app) {
  return app.directive('playGrid', function() {
    return {
      restrict: 'E',
      replace: true,
      template: window.wtc.TEMPLATES.playGrid,
      link: function(scope, el, attrs, controller) {
        var body, html;
        if (attrs.newwindow === "true") {
          scope.newwindow = true;
        }
        if (attrs.omitpager === "true") {
          scope.omitpager = true;
        }
        if (attrs.autoload === "true") {
          scope.autoloadgrid = true;
          body = document.body;
          html = document.documentElement;
          return window.addEventListener('scroll', function(e) {
            var docheight, scrollbottom;
            scrollbottom = window.scrollY + window.innerHeight;
            docheight = Math.max(body.scrollHeight, body.offsetHeight, html.clientHeight, html.scrollHeight, html.offsetHeight);
            if (scrollbottom > docheight - 600) {
              if (scope.data.next_page) {
                return scope.loadMore();
              }
            }
          });
        } else {
          return scope.autoloadgrid = false;
        }
      }
    };
  });
})(window.PlayNintendo.ngApp);


