(function(app, swipe) {
  return app.directive('playAmiiboTab', [
    '$timeout', function(timer) {
      return {
        restrict: 'E',
        replace: true,
        transclude: true,
        scope: {
          headline: '@headline'
        },
        template: window.wtc.TEMPLATES.playRelatedGamesTab,
        link: function(scope, el, attrs) {
          scope.closeText = 'amiibo';
          scope.inputid = 'related-amiibo';
          scope.labelid = 'relatedAmiiboTabLabel';
          scope.type = "amiibo";
          setTimeout(function() {
            var cb, desktopLabel, forProxy, gameCarousel, i, item, label, len, mobileLabel, modal, ref, selector, w, wrapper;
            selector = '#amiibo-carousel';
            gameCarousel = el[0].querySelector(selector);
            if (gameCarousel) {
              modal = document.querySelector('.related-amiibo .content');
              modal.style.display = 'block';
              wrapper = gameCarousel.querySelector('.item-wrapper');
              w = 0;
              ref = wrapper.children;
              for (i = 0, len = ref.length; i < len; i++) {
                item = ref[i];
                w += item.offsetWidth;
              }
              modal.style.display = '';
              wrapper.style.width = w + "px";
              wrapper.setAttribute('data-width', w + "px");
              window.amiiboswiper = new swipe.Swiper(gameCarousel, {
                navContainer: gameCarousel.querySelector('nav'),
                navLeft: gameCarousel.querySelector('.nav-left'),
                navRight: gameCarousel.querySelector('.nav-right')
              });
              label = document.getElementById(scope.labelid);
              forProxy = label.getAttribute('for');
              desktopLabel = label.cloneNode(true);
              desktopLabel.id = 'tabOpenLabelAmiiboDesktop';
              desktopLabel.className = 'tabOpenLabelDesktop';
              desktopLabel.setAttribute('for', '');
              document.getElementById('characterTabs').appendChild(desktopLabel);
              mobileLabel = label.cloneNode(true);
              mobileLabel.id = 'tabOpenLabelAmiiboMobile';
              mobileLabel.className = 'tabOpenLabelMobile relatedAmiiboTabLabel';
              mobileLabel.setAttribute('for', '');
              document.querySelector('.page-theme-character').appendChild(mobileLabel);
              label.parentNode.removeChild(label);
              cb = function(e) {
                var cbox, event, j, labels, len1, name, opening;
                cbox = document.getElementById(forProxy);
                window.cbox = cbox;
                name = document.querySelector('.page-theme h1').innerText;
                labels = document.querySelectorAll('label.tabOpenLabelDesktop');
                opening = this.className.indexOf('open') === -1;
                if (opening) {
                  for (j = 0, len1 = labels.length; j < len1; j++) {
                    label = labels[j];
                    if (label.id === this.id) {
                      continue;
                    }
                    if (label.className.indexOf('open') !== -1) {
                      event = document.createEvent('HTMLEvents');
                      event.initEvent('click', true, false);
                      label.dispatchEvent(event);
                    }
                  }
                }
                setTimeout(function() {
                  return window.amiiboswiper.reRender();
                }, 100);
                if (opening) {
                  window.wtc.Nintendo.Omni.track({
                    6: "see " + name + "'s amiibo"
                  }, [32]);
                  this.className = this.className + ' open';
                  return cbox.checked = true;
                } else {
                  window.wtc.Nintendo.Omni.track({
                    6: "see " + name + "'s amiibo"
                  }, [33]);
                  this.className = this.className.split(' open').join('');
                  return cbox.checked = false;
                }
              };
              desktopLabel.addEventListener('click', cb);
              mobileLabel.addEventListener('click', cb);
            }
          }, 1000);
        }
      };
    }
  ]);
})(window.PlayNintendo.ngApp, window.Swipe);


