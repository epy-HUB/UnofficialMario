(function(_a, _ns, w) {

  /*
    playThemeRelatedBox
    =================================
    This controller loads and controls the friends element on the character pages.
    __Services__
    - *$element* The root element of the associated directive
    - *$scope* The controller scope object
   */
  return _ns.controller('playThemeRelatedBox', [
    '$element', '$scope', '$transclude', function($element, $scope, $transclude) {
      var elements;
      $scope.current = null;
      elements = $transclude($scope, function(notLinkedClone, cloneScope) {
        return $element.append(notLinkedClone);
      });
      $scope.iAmBoxScope = true;
      $scope.tabs = [];
      $scope.registerTab = function(tabName, isCurrent, tabScope, endpoint) {
        var tab;
        if (!$scope.current) {
          isCurrent = true;
          tabScope.current = true;
          $scope.current = tabName;
        }
        tab = $scope.tabs.push({
          tabName: tabName,
          tabScope: tabScope,
          current: isCurrent,
          endpoint: endpoint
        });
      };
      $scope.clickTab = function(index) {
        var i, len, ref, tab;
        ref = $scope.tabs;
        for (i = 0, len = ref.length; i < len; i++) {
          tab = ref[i];
          tab.current = false;
          tab.tabScope.current = false;
        }
        $scope.tabs[index].current = true;
        $scope.tabs[index].tabScope.current = true;
        return setTimeout(function() {
          var event;
          event = document.createEvent('HTMLEvents');
          event.initEvent('resize', true, false);
          return window.dispatchEvent(event);
        }, 100);
      };
      $element[0].style.opacity = 1;
    }
  ]);
})(angular, window.PlayNintendo.ngApp, window);


