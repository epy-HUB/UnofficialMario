        if (loadCallback && typeof loadCallback === 'function') {
          loadCallback();
          loadCallback = null;
        }
      };

      /*
        private **_insertRandomTile**
        -------------------------
        Insert the random tile into a position within the grid
        __Properties__
        - *int* __position__ The position in the list to insert the tile
       */
      randomTileInserted = false;
      _insertRandomTile = function(position) {
        if (position == null) {
