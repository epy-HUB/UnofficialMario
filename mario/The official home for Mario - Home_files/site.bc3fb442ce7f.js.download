  - *Description*     The theme drective
  - *Edited by*       liamegan
  - *Edited*          2015-05-07 14:23:31
  - *Version*         1.0
 */
(function(app) {
  return app.directive('playTheme', function() {
    return {
      restrict: 'A',
      link: function(scope, element, attrs) {
        var _resize;
        _resize = function() {
          var contentHeight, height;
          element[0].style.height = "";
          height = element[0].offsetHeight;
          contentHeight = element[0].querySelector('.content').offsetHeight;
          if (document.querySelector('.page-theme-character__related-box')) {
            contentHeight = contentHeight + 150;
          }
          if (height < contentHeight && window.wtc.responsive.Breakpoints.getBreakpoint() !== 'small') {
            height = contentHeight;
          }
          return element[0].style.height = height + "px";
        };
        window.addEventListener('resize', _resize);
        return window.addEventListener('load', _resize);
      }
    };
  });
})(window.PlayNintendo.ngApp);


/*
    Angular Directive - Play Gallery element
    =======================================
  - *Author*          liamegan
  - *email*           liam@wethecollective.com
  - *Created*         2014-06-09 07:40:42
  - *namespace*       window
  - *Requirements*    n/a
  - *Description*     An angular directive for creating the gallery object
  - *Edited by*       liamegan
  - *Edited*          2015-05-20 17:45:08
  - *Version*         1.0
 */
(function(app, swipe) {
  return app.directive('playActivityGallery', [
    '$timeout', function(timer) {
      return {
        restrict: 'E',
        replace: true,
        transclude: true,
        template: window.wtc.TEMPLATES.playActivityGallery,
        scope: {},
        controller: [
          '$scope', function($scope) {
            var images;
            images = $scope.images = [];
            this.addImage = function(image) {
              var img, j, len;
              images.push(image);
              for (j = 0, len = images.length; j < len; j++) {
                img = images[j];
                img.galleryLength = images.length;
              }
            };
          }
        ],
        link: function(scope, element, attrs) {
          var init;
          init = function() {
            var caption, captiondetail, event, gallery, galleryel, implementedOffset, isDragging, nav, navli, navul, pagenum, positionCaption, switcher, thumbnails;
            pagenum = /([0-9]+)/.exec(window.location.hash);
            if (pagenum && pagenum.length && pagenum.length >= 2) {
              pagenum = Number(pagenum[1]) + 1;
            } else {
              pagenum = 1;
            }
            isDragging = false;
            switcher = element[0];
            galleryel = switcher.querySelector('.gallery-switcher');
            nav = switcher.querySelector('nav.thumbnails');
            navul = nav.querySelector('ul');
            navli = nav.querySelectorAll('li');
            caption = switcher.querySelector('.caption');
            captiondetail = caption.querySelector('.detail');
            scope.length = Number(attrs.length);
            scope.bgcolour = attrs.bgcolour;
            gallery = new swipe.Swiper(galleryel, {
              wrapperSelector: '.gallery-wrapper',
              itemWrapperSelector: '.gallery-images',
              fractionBeforeScroll: 20,
              navContainer: galleryel.querySelector('nav'),
              navLeft: galleryel.querySelector('nav .nav-left'),
              navRight: galleryel.querySelector('nav .nav-right')
            });
            if (pagenum > 1) {
              setTimeout(function() {
                return gallery.gotoPage(pagenum);
              }, 1000);
            }
            implementedOffset = 0;
            gallery.addEventListener('onSwitch', function(e) {
              var altOffset, captionHTML, i, item, j, leftpage, leftpos, len, li, offset, pageNum, rightpage, rightpos, selLi, width;
              selLi = null;
              for (i = j = 0, len = navli.length; j < len; i = ++j) {
                li = navli[i];
                if (i + 1 !== e.detail.page) {
                  if (li.classList) {
                    li.classList.remove('sel');
                  } else {
                    li.className = li.className.split(' sel').join('');
                  }
                } else {
                  selLi = li;
                  if (li.classList) {
                    li.classList.add('sel');
                  } else {
                    li.className += ' ' + 'sel';
                  }
                }
              }
              offset = thumbnails.getOffset(thumbnails.page);
              width = thumbnails.getCarouselWidthOld();
              leftpos = selLi.offsetLeft - navul.offsetLeft;
              rightpos = selLi.offsetLeft - navul.offsetLeft + selLi.offsetWidth;
              rightpage = width + offset;
              leftpage = thumbnails.getOffset(thumbnails.page);
              altOffset = navli[0].offsetWidth / 2;
              if (rightpos > rightpage) {
                pageNum = thumbnails.getPagePosition(rightpos);
                if (pageNum > thumbnails.getPages()) {
                  pageNum = thumbnails.getPages();
                  thumbnails.setPosition(offset + (rightpos - rightpage));
                  implementedOffset = offset + (rightpos - rightpage);
                } else {
                  implementedOffset = thumbnails.gotoPage(pageNum);
                }
              } else if (leftpos < leftpage) {
                pageNum = thumbnails.getPagePosition(leftpos);
                implementedOffset = thumbnails.gotoPage(pageNum);
              }
              if (selLi.offsetLeft < implementedOffset) {
                window.setLi = selLi;
                window.altOffset = altOffset;
                implementedOffset = selLi.offsetLeft - altOffset;
                thumbnails.setPosition(implementedOffset);
              } else if (selLi.offsetLeft + selLi.offsetWidth > implementedOffset + width) {
                implementedOffset = selLi.offsetLeft - width + altOffset * 2.5;
                thumbnails.setPosition(implementedOffset);
              }
              captiondetail.innerHTML = '';
              captionHTML = '';
              item = galleryel.querySelector(".gallery-images .image:nth-child(" + e.detail.page + ") img");
              if (item) {
                captionHTML = item.getAttribute('data-caption');
              }
              if (captionHTML && captionHTML !== '') {
                caption.className = 'caption';
                captiondetail.innerHTML = captionHTML;
              } else {
                caption.className = 'caption hidden';
              }
              positionCaption();
            });
            positionCaption = function() {
              caption.style.marginTop = "-" + caption.offsetHeight + "px";
            };
            thumbnails = new swipe.Swiper(nav, {
              wrapperSelector: '.swiper',
              itemWrapperSelector: 'ul'
            });
            thumbnails.getCarouselWidthOld = thumbnails.getCarouselWidth;
            thumbnails.getCarouselWidth = function() {
              var w;
              return w = this.getCarouselWidthOld();
            };
            thumbnails.getPagePosition = function(pos) {
              var havePage, page, rightPosition, testPage;
              if (pos == null) {
                pos = 0;
              }
              page = 1;
              testPage = 1;
              havePage = false;
              while (!havePage) {
                rightPosition = this.getCarouselWidthOld() + this.getOffset(testPage);
                if (pos < rightPosition) {
                  page = testPage;
                  havePage = true;
                }
                testPage++;
                if (testPage === 100) {
                  break;
                }
              }
              return page;
            };
            thumbnails.getOffsetOld = thumbnails.getOffset;
            thumbnails.getOffset = function(pageNum) {
              var altOffset, el, newOffset, oldOffset, sel;
              el = this.el;
              sel = el.querySelector('.sel');
              oldOffset = this.getOffsetOld(pageNum);
              altOffset = navli[0].offsetWidth + navli[0].offsetWidth / 4;
              if (pageNum === 1) {
                altOffset = 0;
              }
              newOffset = (this.getCarouselWidth() * (pageNum - 1)) - altOffset;
              return newOffset;
            };
            window.addEventListener('resize', function(e) {
              var BP, firstLi, j, len, li, lilength, lis, width;
              BP = window.wtc.responsive.Breakpoints.getBreakpoint();
              firstLi = navul.querySelector('li:first-child');
              lis = navul.children;
              lilength = lis.length;
              if (BP === 'medium') {
                width = nav.offsetWidth / 4.5;
              } else {
                width = nav.offsetWidth / 6.5;
              }
              for (j = 0, len = lis.length; j < len; j++) {
                li = lis[j];
                li.style.width = width + "px";
              }
              navul.style.width = (Math.ceil(lilength * width)) + "px";
              positionCaption();
              gallery.gotoPage(null);
            });
            event = document.createEvent('HTMLEvents');
            event.initEvent('resize', true, false);
            window.dispatchEvent(event);
            scope.select = function(index) {
              if (isDragging) {
                return;
              }
              gallery.gotoPage(index + 1);
            };
          };
          timer(init, 0);
        }
      };
    }
  ]).directive('playGalleryImage', function() {
    return {
      require: '^playActivityGallery',
      restrict: 'E',
      replace: true,
      transclude: true,
      template: window.wtc.TEMPLATES.playActivityGalleryImage,
      scope: {
        src: "@src",
        thumb: "@thumb",
        caption: "@caption"
      },
      link: function(scope, element, attrs, galleryCtrl) {
        galleryCtrl.addImage(scope);
        scope.getItemWidth = function() {
          var percent;
          percent = 100 / scope.galleryLength;
          return percent + "%";
        };
      }
    };
  });
})(window.PlayNintendo.ngApp, window.Swipe);


