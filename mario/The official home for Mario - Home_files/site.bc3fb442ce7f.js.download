          if (data.results && data.results.constructor === Array) {
            data.results.forEach(function(record) {
              return $scope.data.results.push(record);
            });
          }
          hasRandomTile = false;
          if (includeSurprise) {
            hasRandomTile = Math.random() < 0.2;
          }
          if (hasRandomTile) {
            randomTilePosition = Math.floor(Math.random() * data.results.length);
            _insertRandomTile(randomTilePosition);
          }
        }
