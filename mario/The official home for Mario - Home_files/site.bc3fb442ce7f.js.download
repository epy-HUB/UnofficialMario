        }
        return styleStr;
      };

      /*
        public **getVerbForItem**
        -------------------------
        Get the verb for the item. The verb is a text description for the content type. It now just refers to the content type specifically, but it was, at one point, an actual verb.
        __Properties__
        - *object* __item__ The tile record to get the verb for.
        __Returns__
        - *string* __verb__ The verb
       */
      $scope.getVerbForItem = function(item) {
        var verb;
        verb = '';
        if (item.content && item.content.type && $scope.types[item.content.type] && $scope.types[item.content.type].verb) {
          verb = $scope.types[item.content.type].verb;
        } else if (item.icon && $scope.types[item.icon] && $scope.types[item.icon].verb) {
          verb = $scope.types[item.icon].verb;
        }
        return verb;
      };

      /*
        public **getHREFForItem**
