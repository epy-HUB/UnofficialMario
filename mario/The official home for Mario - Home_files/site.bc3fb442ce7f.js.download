  - *Requirements*    Angular, w.PlayNintendo.CONST
  - *Description*     This is a controller
  - *Edited by*       Liam Egan
  - *Edited*          2016-05-11 10:02:49
  - *Version*         1.0
 */
var base, base1, w;

w = window;

w.PlayNintendo || (w.PlayNintendo = {});

(base = w.PlayNintendo).app || (base.app = {});

(base1 = w.PlayNintendo.app).controllers || (base1.controllers = {});

(function(_a, _ns, w) {
  var _JSON, dataready;
  _JSON = null;
  dataready = false;

  /*
    __playNintendoContentController__
    =================================
    The content controller builds out the tile grid from the content API service
    __Services__
    - *$scope* The controller scope object
    - *$http* The HTTP method for loading the content over HTTP
    - *$filter*
    - *$attrs* The attributes of the angular element
    - *$element* The HTML element for the grid
    - *$rootScope* The root scope
   */
  _ns.playNintendoContentController = [
    '$scope', '$http', '$filter', '$attrs', '$element', '$rootScope', function($scope, $http, $filter, $attrs, $element, $rootScope) {
      var _assembleParams, _insertRandomTile, _load, _load_fresh, _parse, _setPage, loadCallback, randomTileInserted, randomTilePosition;
      _JSON = $attrs.endpoint;
      console.log(_JSON, $attrs);
      $scope.gridparams = {
        format: 'json'
      };
      if ($attrs.pagesize && !isNaN($attrs.pagesize)) {
        $scope.gridparams.page_size = Number($attrs.pagesize);
      }
      if ($attrs.tiletype) {
        $scope.gridparams.tiletype = $attrs.tiletype;
      }
      $scope.data = [];
      $scope.pageNum = 1;
      $scope.types = $rootScope.types;
      $scope.filters = [];
      randomTilePosition = 0;
      loadCallback = null;

      /*
        private **_assembleParams**
        -------------------------
        Assembles the parameters for the loading of the content from the API
        __Properties__
        - *object* ___params__ The parameters to extend with
       */
      _assembleParams = function(_params) {
        if (_params == null) {
          _params = {};
        }
        window.wtc.utility.deepExtend($scope.gridparams, _params);
        return $scope.gridparams;
      };

      /*
        private **_load**
        -------------------------
        Load the data from the API using the HTTP GET method
        __Properties__
        - *string* __URL__ The URL of the API endpoint
        - *obj* __params__ The parameters to use for the call
        - *boolean* __isFirstPush__ Whether this is the first set of loaded data
        - *function* __callbackOnComplete__ The function to call when the load has completed
       */
      _load = function(URL, params, isFirstPush, callbackOnComplete) {
        if (URL == null) {
          URL = "" + w.PlayNintendo.CONST.API_ROOT + _JSON;
        }
        if (params == null) {
          params = {};
        }
        if (isFirstPush == null) {
          isFirstPush = false;
        }
        if (callbackOnComplete == null) {
          callbackOnComplete = null;
        }
        if ($scope.loading === true) {
          return;
        }
        if (callbackOnComplete && typeof callbackOnComplete === 'function') {
          loadCallback = callbackOnComplete;
        }
        $scope.loading = true;
        return $http.get(URL, {
          params: _assembleParams(params)
        }).success(function(data) {
          return _parse(data, isFirstPush);
        });
      };

      /*
        public **load**
        -------------------------
        Load the data from the API using the private _load method
       */
      $scope.load = function() {
        _load("" + w.PlayNintendo.CONST.API_ROOT + _JSON, {}, true);
      };
      _setPage = function(page) {
        var _params;
        $scope.pageNum = page;
        _params = {
          page: $scope.pageNum
        };
        return _params;
      };

      /*
        public **loadMore**
        -------------------------
        Load more data from the API using the private _load method
        __Properties__
        - *function* __callbackOnComplete__ The function to call when the load has completed
       */
      $scope.loadMore = function(callbackOnComplete) {
        var _params;
        if ($scope.loading === true) {
          return;
        }
        if (!$scope.data.next_page) {
          return;
        }
        _params = _setPage($scope.pageNum + 1);
        _load("" + w.PlayNintendo.CONST.API_ROOT + _JSON, _params, false, callbackOnComplete);
      };

      /*
        private **_parse**
        -------------------------
        Parse the data loaded from the API
        __Properties__
        - *object* __data__ The data to parse
        - *boolean* __isFirstPush__ Whether this is the first set of loaded data
        - *boolean* __includeSurprise__ Whether to include the surprise block in the layout
       */
      _parse = function(data, isFirstPush, includeSurprise) {
        var container, halfway, hasRandomTile, i;
        if (isFirstPush == null) {
          isFirstPush = false;
        }
        if (includeSurprise == null) {
          includeSurprise = true;
        }
        container = document.querySelector('[isotope-container]');
        setTimeout(function() {
          var sizer;
          if (container && container.offsetHeight) {
            sizer = document.querySelector('.sizer');
            if (container.offsetHeight < sizer.offsetWidth * 2) {
              return container.style.minHeight = (sizer.offsetWidth * 2) + 'px';
            } else {
              return container.style.minHeight = container.offsetHeight + 'px';
            }
          }
        }, 500);
        i = 1;
        if (data.results && data.results.constructor === Array) {
          data.results.forEach(function(record) {
            if ($attrs.force1x1 && $attrs.force1x1 === 'true') {
              record.type = 'one';
            }
            record.subposition = ['l', 'r'][Math.floor(Math.random() * 2)];
            record.direction = ['h', 'v'][Math.floor(Math.random() * 2)];
            if ($attrs.force2x1 && $attrs.force2x1 === 'true') {
              if (record.type === 'featured') {
                record.image = record.profile_image;
              }
              record.type = 'two';
              record.direction = 'h';
            }
            record.index = i++;
            record.stagger = Math.floor(Math.random() * 6);
            if (record.content.type.indexOf('Theme') === 0) {
              if (record.content.type === "Theme.Character" || record.type === 'one') {
                record.rootType = 'theme';
              } else {
                record.rootType = 'tile';
              }
            } else {
              record.rootType = 'tile';
            }
            if (!record.colour || record.colour === '') {
              return record.colour = 'transparent';
            }
          });
        } else {
          data.results = [];
        }
        if (isFirstPush) {
          $scope.data = data;
          halfway = $scope.data.results.length / 2;
          randomTilePosition = Math.floor(halfway + (Math.random() * halfway));
          if (includeSurprise && !$attrs.omitsurprise) {
