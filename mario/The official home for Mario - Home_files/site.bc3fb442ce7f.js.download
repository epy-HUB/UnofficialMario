                chromeless: _chromeless
              });
              window._video = this._video;
              return setTimeout((function(_this) {
                return function() {
                  console.log(_this._video.player);
                  return _this._video.player.subscribe('*', '', function(name) {
                    if (window.kiosk) {
                      return window.kiosk.resetFunction();
